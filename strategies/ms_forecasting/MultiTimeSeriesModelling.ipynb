    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def plotPredictedVsActual(predictions_input, instrument):\\n\",\n",
    "    \"    df_preds = predictions_input.copy().rename(columns={\\\"index\\\": \\\"time\\\"})\\n\",\n",
